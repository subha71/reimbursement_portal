<h2>Create a New Company</h2>

<%= form_with model: @company, local: true do |form| %>
  <div class="mb-3">
    <%= form.label :name %>
    <%= form.text_field :name, class: 'form-control', required: true, autocomplete: 'off' %>
  </div>

  <%= form.submit 'Create Company', class: 'btn btn-primary' %>
<% end %>